{
  "name": "shiki-extension",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack -p",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "serve": "node express -p 80",
    "dev": "npm-run-all -p serve start",
    "bump-version": "node ../../scripts/bump-version.js",
    "create-release": "node ../../scripts/create-release.js",
    "package": "npm run build && cd ../dist && zip -r ../shiki-extension.zip . -x \"*.map\"",
    "clean": "rm -rf dist/* && rm -f ../shiki-extension.zip",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.1.0",
    "highlight-within-textarea": "^2.0.5",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "popper.js": "^1.15.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.9",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.2",
    "windows-1251": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "express": "^4.17.1",
    "minimist": "^1.2.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.9",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "eslint": "^8.0.0"
  }
}
