{
  "name": "@shiki-extender/extension",
  "version": "1.0.0",
  "description": "Chrome extension for Shiki Extender",
  "main": "src/background.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=production",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "serve": "node src/express -p 80",
    "dev": "npm-run-all -p serve start",
    "bump-version": "node ../../scripts/bump-version.js",
    "create-release": "node ../../scripts/create-release.js",
    "package": "npm run build && cd dist && zip -r ../shiki-extension.zip . -x \"*.map\"",
    "clean": "rm -rf dist/* && rm -f shiki-extension.zip",
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "eslint src --ext .js,.vue --fix",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@traptitech/markdown-it-katex": "^3.6.0",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-vue": "^2.23.1",
    "highlight-within-textarea": "^2.0.5",
    "jquery": "^3.7.1",
    "markdown-it": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "portal-vue": "^3.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "windows-1251": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@vue/compiler-sfc": "^3.5.13",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.16.0",
    "express": "^4.21.1",
    "minimist": "^1.2.8",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "style-loader": "^4.0.0",
    "stylus": "^0.63.0",
    "stylus-loader": "^8.1.0",
    "vue-loader": "^17.4.2",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}